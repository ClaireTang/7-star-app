
.member-top {
	position: relative;
	width: 100%;
	height: 200rpx;
	/* background-color: #FF7159; */
}
.bg-img {
	position: absolute;
	width: 100%;
	height: 100%;
}
.member-top-c {
	position: absolute;
	top: 50%;
	left: 50rpx;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	/* text-align: center; */
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.user-head-img {
	display: block;
	width: 100rpx;
	height: 100rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	overflow: hidden;
	background-color: rgba(255, 255, 255, 0.7);
	margin-right: 20rpx;
}
.user-name {
	font-size: 30rpx;
	color: #fff;
}
.member-grid {
	background-color: #fff;
	/* border-top: 2upx solid #eee; */
	padding: 20rpx 0;
}
.member-item{
	padding: 0 26rpx;
	text-align: left;
}
.margin-cell-group {
	margin: 20rpx 0;
	color: #666666;
}
.badge {
	left: 80rpx;
	top: -6rpx;
}
button.cell-item-hd {
	background-color: #fff;
	padding: 0;
	line-height: 1.4;
	color: #333;
}
button.cell-item-hd:after {
	border: none;
}
.login-btn {
	color: #fff;
	width: 160rpx;
	height: 50rpx;
	line-height: 50rpx;
	-webkit-border-radius: 25rpx;
	        border-radius: 25rpx;
	background: #ff7159;
	font-size: 12px;
	margin-top: 16rpx;
}
.dist-list{
	overflow: hidden;
	background-color: #fff;
	/* padding: 26rpx 26rpx 0; */
	margin: 0 26rpx;
	border-bottom: 2rpx solid #eee;
}
.dist-item{
	width: 50%;
	text-align: left;
	float: left;
	height: 120rpx;
	margin-top: 20rpx;
}

